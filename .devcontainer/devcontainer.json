{
	"name" : "Dev Container for Template Project",
	"build" : {
		"dockerfile" : "Dockerfile"
	},
	"runArgs": ["--platform=linux/amd64" ],
	"containerUser": "vscode",
	"customizations" : {
		"vscode" : {
			"extensions" : [
				"ms-vscode.cpptools-extension-pack",
				"ms-python.python",
				"mcu-debug.debug-tracker-vscode",
				"marus25.cortex-debug"
			]
		}
	}
}